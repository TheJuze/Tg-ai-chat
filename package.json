{
  "name": "tg-ai-chat",
  "version": "1.0.0",
  "description": "Telegram bot with OpenAI integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "nodemon --exec ts-node src/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test:connection": "node scripts/test-connection.js"
  },
  "keywords": [
    "telegram",
    "bot",
    "openai",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@openai/agents": "^0.0.13",
    "dotenv": "^16.3.1",
    "openai": "^4.20.1",
    "telegraf": "^4.15.6",
    "winston": "^3.11.0",
    "zod": "<=3.25.67"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
